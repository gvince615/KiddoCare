<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="10dp">

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginRight="10dp"
        android:text="Meal Name:"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/tv_D_MealName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView18"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="15dp"
        android:layout_toEndOf="@+id/textView18"
        android:clickable="true"
        android:hint="Enter Meal Name"
        android:onClick="createAlert"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView18"
        android:layout_marginTop="25dp"
        android:text="Tap below to Select (or add) Dinner Meal Items:"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/tv_D_Drink"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView19"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView19"
        android:layout_marginLeft="15dp"
        android:clickable="true"
        android:hint="Select Drink Item"
        android:onClick="createAlert"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/tv_D_Bread"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/tv_D_Drink"
        android:layout_marginLeft="15dp"
        android:clickable="true"
        android:hint="Select Bread Item"
        android:onClick="createAlert"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/tv_D_ForV1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/tv_D_Bread"
        android:layout_marginLeft="15dp"
        android:clickable="true"
        android:hint="Select Fruit or Veggie Item 1"
        android:onClick="createAlert"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/tv_D_Protein"
        android:layout_marginTop="24dp"
        android:text="Tap below to Select (or add) Night Snack Items:"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/tv_DMS_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/tv_D_MealName"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView23"
        android:layout_marginLeft="15dp"
        android:clickable="true"
        android:hint="Select Any Food Item 1"
        android:onClick="createAlert"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/tv_DMS_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/tv_D_MealName"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/tv_DMS_1"
        android:layout_marginLeft="15dp"
        android:clickable="true"
        android:hint="Select Any Food Item 2"
        android:onClick="createAlert"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/tv_D_ForV2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/tv_D_ForV1"
        android:layout_marginLeft="15dp"
        android:clickable="true"
        android:hint="Select Fruit or Veggie Item 2"
        android:onClick="createAlert"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/tv_D_Protein"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/tv_D_MealName"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/tv_D_ForV2"
        android:layout_marginLeft="15dp"
        android:clickable="true"
        android:hint="Select Protein Item"
        android:onClick="createAlert"
        android:textAppearance="?android:attr/textAppearanceLarge" />

</RelativeLayout><!-- From: file:/Users/gvincent/Documents/studioprojects/KiddoCare/app/src/main/res/layout/fragment_dinner.xml -->